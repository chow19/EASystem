<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.EaSystem.mapper.ICalendarofWeekService">
	<insert id="setCofW" >
		insert into calendar values(#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},#{param8},#{param9})
	</insert>
	
	<select id="getExCalendarofWeek" resultType="com.EaSystem.beans.CalendarofWeek">
		select * from calendar where (weekname=(select max(weekName) from calendar)) or (weekname=(select min(weekName) from calendar))
	</select>
	
	<select id="getCalendarofWeek" resultType="com.EaSystem.beans.CalendarofWeek">
		select * from calendar
	</select>
</mapper>