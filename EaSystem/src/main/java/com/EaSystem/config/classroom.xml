<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.EaSystem.mapper.IClassroomService">
	<select id="getAll" resultType="com.EaSystem.beans.Classroom">
		select * from classroom
	</select>
	
	<select id="getClassroom" resultType="com.EaSystem.beans.Classroom">
		select * from classroom where id=#{param1}
	</select>
	
</mapper>